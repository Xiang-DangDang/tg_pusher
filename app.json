    "SITE_URL": {
      "description": "访问域名地址",
      "value": "https://tg-pusher.herokuapp.com",
      "required": false
    }
  },
  "success_url": "/",
  "website": "https://tg-pusher.herokuapp.com/",
  "repository": "https://github.com/Xiang-DangDang/tg_pusher",
  "stack": "heroku-20"
}