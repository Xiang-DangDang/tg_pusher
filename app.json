{
  "name": "tg_pusher",
  "description": "Deploy tg_pusher on Heroku.",
  "keywords": [
    "tg_pusher"
  ],
  "env": {
    "TG_BOT_TOKEN": {
      "description": "机器人 token",
      "value": ""
    },
    "SECRET_ID": {
      "description": "随机加密ID",
